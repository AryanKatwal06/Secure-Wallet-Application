{
 "expo": {
 "name": "SecureWallet",
 "slug": "SecureWallet",
 "version": "1.0.0",
 "orientation": "portrait",
 "icon": "./assets/logoW.jpeg",
 "scheme": "frontend",
 "userInterfaceStyle": "automatic",
 "newArchEnabled": true,
 "updates": {
 "enabled": false
 },
 "ios": {
 "supportsTablet": true
 },
 "android": {
 "adaptiveIcon": {
 "foregroundImage": "./assets/logoW.jpeg",
 "backgroundColor": "#0f172a"
 },
 "edgeToEdgeEnabled": true,
 "predictiveBackGestureEnabled": false
 },
 "web": {
 "output": "static",
 "favicon": "./assets/logoW.jpeg"
 },
 "plugins": [
 "expo-router",
 [
 "expo-splash-screen",
 {
 "image": "./assets/logoW.jpeg",
 "imageWidth": 200,
 "resizeMode": "contain",
 "backgroundColor": "#0f172a",
 "dark": {
 "backgroundColor": "#000000"
 }
 }
 ]
 ],
 "experiments": {
 "typedRoutes": true,
 "reactCompiler": true
 }
 }
}